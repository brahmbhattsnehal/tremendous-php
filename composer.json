{
    "name": "hthabet/tremendous-php",
    "description": "PHP SDK for Tremendous API",
    "type": "library",
    "require": {
        "php": "^7.1",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/http-foundation": "2.8.*|3.1.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "fzaninotto/faker": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Tremendous\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
